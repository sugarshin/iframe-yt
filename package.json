{
  "name": "youtube-thin-wrapper",
  "version": "1.0.0",
  "description": "a YouTube iframe API thinness wrapper",
  "repository": {
    "type": "git",
    "url": "git@github.com:sugarshin/youtube-thin-wrapper.git"
  },
  "main": "index.js",
  "files": [
    "lib",
    "index.js",
    "test",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "youtube",
    "iframe",
    "browser"
  ],
  "scripts": {
    "lint": "eslint src",
    "start": "npm run compile -- -w",
    "test": "mocha --timeout 3000 --compilers js:espower-babel/guess test/**/*.js",
    "compile": "babel --stage 0 -d lib src",
    "build": "npm run lint && npm run test && npm run compile"
  },
  "engines": {
    "node": "4.1.x"
  },
  "author": "sugarshin",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "eslint": "^1.5.1",
    "espower-babel": "^3.3.0",
    "jsdom": "^6.5.1",
    "mocha": "^2.3.3",
    "power-assert": "^1.0.1"
  },
  "dependencies": {
    "deep-extend": "^0.4.0"
  }
}
